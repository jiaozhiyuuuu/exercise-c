#include <stdio.h> 
#include <string.h>
int main()
{
	int i;
	char password[20] = "0";
	for(i=0;i<3;i++)
	{
		printf("请输入你的密码:>\n");
		scanf("%s\n",&password);
		if(strcmp(password,"123456")==0)
			{
			printf("密码正确\n");
			break;
			}
		else
			printf("密码错误\n");		
	}
	if(i==3)
		printf("别回了\n");	
	return 0;
}
